#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	float p = -1, pmax = -1;
	int i, K=0, N, M, min=INT_MAX, max=INT_MIN, c = 0, j, j1, c1=0, s = 0; 
	float m;
	cin >> N>>M;
	int *A[N];
	int *B=new int[M];
	for	(i=0; i<M; i++) A[i] = new int[N];
	cout << "Vvedite elementy matricy: ";
	for (i = 0; i < M; i++){
		for (j = 0; j < N; j++)
			cin >> A[i][j];
	}
	
	for (i=0; i<M; i++){
	 B[i]=A[i][0];				
		}
	int tmp = 0;
  	for(i = 0;i<M;i++){
    for(j = (M-1);j>=(i+1);j--){
      if(B[j]<B[j-1]){
        tmp = B[j];
        B[j]=B[j-1];
        B[j-1]=tmp;
      }
    }
  }
	for (i=0; i<M; i++){
		if (A[i][0]!=B[i]){
		
		for (c=0; c<M; c++) {
			if (A[i][0]==B[c]){
				for (j=0; j<N; j++){
					A[c][j]+=A[i][j];
					A[i][j]=A[c][j]-A[i][j];
					A[c][j]-=A[i][j];
		
	}}
		
	
		}
	}
	}
	for (i=0; i<M; i++){
		for (j = 0; j < N; j++){
			
			cout<<A[i][j]<<"\t";
		}
		cout<<endl;
}
}




